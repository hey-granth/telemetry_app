# Environment Configuration
# Copy this file to .env and update values for your environment

# Application
APP_ENV=development
SECRET_KEY=your-secret-key-change-in-production
LOG_LEVEL=debug

# Database
POSTGRES_USER=telemetry
POSTGRES_PASSWORD=telemetry_secret
POSTGRES_DB=telemetry_db
POSTGRES_PORT=5432
DATABASE_URL=postgresql+asyncpg://telemetry:telemetry_secret@localhost:5432/telemetry_db

# API
API_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://localhost:8080

# pgAdmin (optional)
PGADMIN_EMAIL=admin@telemetry.local
PGADMIN_PASSWORD=admin
PGADMIN_PORT=5050
